    <event eventtype="3" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handles tux's sprites

//Disable ducking for certain powerups
if global.powerup = cs_small
or global.powerup = cs_frog
    isduck = 0

//If tux is not ducking
if isduck = 0
{

    //Select Mask
    if global.powerup = cs_small
        mask_index = spr_smallmask
    else
        mask_index = spr_bigmask

    //Shoot Animation
    if global.powerup = cs_fire
    or global.powerup = cs_hammer
    or global.powerup = cs_bomb
    or global.powerup = cs_ice
    or global.powerup = cs_super
    or global.powerup = cs_ninja
    or global.powerup = cs_penguin
    or global.powerup = cs_boomerang
    {
        if firing &gt; 0
            firing--
    }
    else
        firing = 0

    //Tail Wiggling
    if global.powerup = cs_cape
    or global.powerup = cs_tanooki
    {
        if state = 2
        and stompstyle = 0
        {
            if wiggle &gt; 1
                wiggle--
            if wiggle &gt; 12
                image_index = 3
            else if wiggle &gt; 8
                image_index = 2
            else if wiggle &gt; 4
                image_index = 1
            else if wiggle &gt; 1
                image_index = 2
            else if wiggle = 1
                image_index = 1
            else
                image_index = 0
        }
        else
            wiggle = 0
    }
    else
        wiggle = 0

    //Not Holding Anything
    if holding = 0
    {

        //Standing State
        if state = 0
        {

            //Shooting Projectiles
            if firing &gt; 0
            {
                image_speed = 0
                if firing &lt; 6 and firing &gt; 3
                    image_index = 1
                else
                    image_index = 0
                sprite_index = scr_tuxshoot()
            }

            //Default Standing
            else
            {
                image_speed = 0
                image_index = 0
                sprite_index = scr_tuxwalk()
            }

        }

        //Walking State
        else if state = 1
        {

            //Animate based on horizontal speed
            image_speed = hspeed/10

            //Frog tux only walks
            if global.powerup = cs_frog
                sprite_index = scr_tuxwalk()

            //Skid when moving and facing in opposite directions
            else if (direct = 1 and hspeed &lt; -0.001)
            or (direct = -1 and hspeed &gt; 0.001)
            {
                sprite_index = scr_tuxskid()
                if skidnow = 0
                {
                    skidnow = 1
                    audio_play_sound(snd_skid,1,1)
                }
            }

            //Default Walking
            else
            {

                //Run only when P-Meter is full
                if obj_levelmanager.pmeter &lt; 6
                    sprite_index = scr_tuxwalk()
                else
                    sprite_index = scr_tuxrun()

            }

        }

        //Jumping State
        else if state = 2
        {

            //Swimming
            if isswim = 1
            {

                //If frog/penguin tux is not swimming
                if noisy = 0
                {
                    sprite_index = scr_tuxswim()
                    image_speed = 0
                    if vspeed &lt; 0
                        image_index = 1
                    else
                        image_index = 0
                }

            }

            //Spin Jumping
            else if stompstyle = 1
            {

                //Propeller tux Flying
                if global.powerup = cs_propeller
                {

                    //Propeller tux floating down slowly
                    if vspeed &gt; 0
                    and vspeed &lt; 2
                    {
                        image_speed = 0.3
                        sprite_index = spr_tux_propeller_walk
                    }

                    //Default propeller tux flying
                    else
                    {
                        image_speed = vspeed/10
                        sprite_index = spr_tux_propeller_fly
                    }

                }

                //Default Spin Jumping
                else
                {
                    image_speed = 0.5
                    sprite_index = scr_tuxwarp()
                }

            }

            //Shooting Projectiles
            else if firing &gt; 0
            {
                image_speed = 0
                if firing &lt; 6 and firing &gt; 3
                    image_index = 1
                else
                    image_index = 0

                //Penguin tux Hotfix
                if global.powerup = cs_penguin
                    sprite_index = spr_tux_penguin_shoot2
                else
                    sprite_index = scr_tuxswim()

            }

            //Default Jumping
            else
            {

                //Floating Animation
                if global.powerup = cs_carrot
                or global.powerup = cs_bee
                {
                    if isfloating = 1
                        image_speed = 0.15
                    else
                        image_index = 0
                }

                //Run only when P-Meter is full
                if obj_levelmanager.pmeter &lt; 6
                    sprite_index = scr_tuxjump()
                else
                    sprite_index = scr_tuxrunjump()

            }

        }

        //Special Swimming Animations
        if global.powerup = cs_frog
        or global.powerup = cs_penguin
        {
            if isswim = 1
            {
                if swimtype = 0
                    sprite_index = scr_tuxswim()
                if swimtype = 1
                    sprite_index = scr_tuxswim2()
                if swimtype = 2
                    sprite_index = scr_tuxswim3()
                if noisy = 1
                    image_speed = 0.15
                else
                    image_speed = 0
            }
        }

    }

    //Holding Something
    else
    {

        //Standing State
        if state = 0
        {
            image_speed = 0
            image_index = 0
        }

        //Walking State
        if state = 1
            image_speed = hspeed/10

        //Jumping State
        if state = 2
        {
            image_speed = 0
            if global.powerup = cs_small
                image_index = 1
            else
                image_index = 2
        }

        //Carrying an item above his head
        if holding = 1
            sprite_index = scr_tuxcarry()

        //Holding an item on front of him
        else if holding = 2
            sprite_index = scr_tuxhold()

    }

    //Climbing
    if state = 3
    {
        if speed = 0
            image_speed = 0
        else
            image_speed = 0.15
        sprite_index = scr_tuxclimb()
    }

    //Wall Climbing + Wall Jumping
    else if state = 4
    {

        //Ninja tux Wall Climbing
        if global.powerup = cs_ninja
        {
            sprite_index = spr_tux_ninja_wallclimb
            if vspeed = 0
                image_speed = 0
            else
                image_speed = 0.15
        }

        //Wall Jumping
        else
        {
            sprite_index = scr_tuxskid()
            if skidnow = 0
            {
                skidnow = 1
                audio_play_sound(snd_skid,1,1)
            }
        }

    }

    //Kicking Animation
    if iamkicking = 1
        sprite_index = scr_tuxkick()

}

//If tux is ducking
else if isduck = 1
{

    //Use Small Mask
    mask_index = spr_smallmask

    //Default Ducking
    sprite_index = scr_tuxduck()

}

//Stop the skid sound effect
if sprite_index != scr_tuxskid()
and skidnow = 1
{
    audio_stop_sound(snd_skid)
    skidnow = 0
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
