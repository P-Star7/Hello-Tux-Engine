{

    //Make tux temporarily invincible
    event_user(6)

    //If tux is not riding anything
    if global.yoshi = 0
    {

        //If tux is small
        if global.powerup = cs_small
        {

            //Kill tux
            instance_create(x,y,obj_deadtux)
            instance_destroy()

        }

        //If tux is not small
        else
        {

            //If tux is big
            if global.powerup = cs_big
            {

                //Make him small
