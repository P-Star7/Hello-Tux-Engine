            with obj_yoshi event_user(1)

        //Get off of a shoe if he is on one
        else if global.yoshi = 2
            with obj_kuriboshoe event_user(0)

    }

    //Play the jump sound if he is not spin jumping
    else
        audio_play_sound(snd_jump,0,0)

    //Enable variable jumping
    jumpnow = 1

    //Switch to the jump state
    state = 2

}

//Check if tux should still be variable jumping
if keyboard_check_released(vk_shift)
and jumpnow = 1
    jumpnow = 2

//If tux is ducking on the ground
if isduck = 1 and state != 2
{

    //Slow down tux's horizontal speed
    if hspeed &gt; 0
        hspeed-=0.05
    if hspeed &lt; 0
        hspeed+=0.05

    //If tux is slow enough, stop his horizontal speed entirely
    if abs(hspeed) &lt; 0.05
        hspeed = 0

}

//If tux is not ducking on the ground
else
{

    //Handles moving right
    if keyboard_check(vk_right)
    and not keyboard_check(vk_left)
    and disablecontrols = 0
    {

        //If tux is not at his maximum speed
        if hspeed &lt; hspeedmax
        {

            //Accelerate slower if he is on a slippery surface
            if isslip = 1
                hspeed += 0.04

            //Default acceleration
            else
            {

                //Accelerate more if he is skidding
                if hspeed &lt; 0
                    hspeed += 0.075

                //Accelerate
                hspeed += 0.06

            }
        }

        //Change tux's direction if he is not wall jumping
        if walljump = 0
            direct = 1

    }

    //Handles moving left
    else if keyboard_check(vk_left)
    and not keyboard_check(vk_right)
    and disablecontrols = 0
    {

        //If tux is not at his maximum speed
        if hspeed &gt; -hspeedmax
        {

            //Accelerate slower if he is on a slippery surface
            if isslip = 1
                hspeed -= 0.04

            //Default acceleration
            else
            {

                //Accelerate more if he is skidding
                if hspeed &gt; 0
                    hspeed += -0.075

                //Accelerate
                hspeed += -0.06

            }
        }

        //Change tux's direction if he is not wall jumping
        if walljump = 0
            direct = -1
    }

    //Takes care of slowing tux down if he is not jumping and left/right is not being pressed
    else if state != 2
    {

        //If tux is not on a slippery surface
        if isslip = 0
        {

            //Slow down tux's horizontal speed
            if hspeed &gt; 0
                hspeed-=0.05
            else if hspeed &lt; 0
                hspeed+=0.05

            //If tux is slow enough, stop his horizontal speed entirely
            if abs(hspeed) &lt; 0.05
                hspeed = 0

        }
        else
        {

            //Slow down tux's horizontal speed
            if hspeed &gt; 0
                hspeed-=0.025
            else if hspeed &lt; 0
                hspeed+=0.025

            //If tux is slow enough, stop his horizontal speed entirely
            if abs(hspeed) &lt; 0.025
                hspeed = 0

        }
    }

    //Slow tux down if he is faster than his maximum speed
    if state != 2
    and abs(hspeed) &gt; hspeedmax
    {

        //Slow down tux's horizontal speed
        if hspeed &gt; 0
            hspeed-=0.025
        else if hspeed &lt; 0
            hspeed+=0.025

        //If tux is slow enough, stop his horizontal speed entirely
        if abs(hspeed) &lt; 0.025
            hspeed = 0
    }

}

//If tux is jumping
if state = 2
or statedelay &gt; 0
{

    //Variable jumping
    if vspeed &gt; -1
    or jumpnow != 1
    {

        //Normal gravity
        gravity = 0.3

        //End variable jumping if it never ends manually
        if jumpnow = 1
            jumpnow = 2

    }

    //Lower gravity while in the middle of variable jumping
    else
        gravity = 0.1

    //Propeller tux jumping
    if global.powerup = cs_propeller
    {

        //If tux is spin jumping normally
        if stompstyle = 1
        and global.yoshi = 0
        {

            //Lower the gravity
            gravity = gravity/2

            //Allow tux to charge downwards
            if keyboard_check(vk_down)
                vspeed = 4
            else if vspeed &gt; 1
                vspeed = 1

            //Play the sound when he charges downwards
            if keyboard_check_pressed(vk_down)
                audio_play_sound(snd_spin,0,0)

        }

    }

}

//Makes tux start climbing
if place_meeting(x,y,obj_climb)
and (keyboard_check(vk_up) or (keyboard_check(vk_down) and state = 2))
and holding = 0
and disablecontrols = 0
{

    //Change to climb state
    state = 3

    //Stop movement
    gravity = 0
    speed = 0

    //Disable carrot and bee tux floating
    isfloating = 0

    //Reset wall jump
    walljump = 0

    //Stop carrot/bee tux's floating sound
    if floatnow = 1
    {
        audio_stop_sound(snd_spin)
        floatnow = 0
    }

}

//Handles sliding down slopes and shell/penguin tux sliding
if keyboard_check_pressed(vk_down)
and disablecontrols = 0
and holding = 0
{

    //If shell/penguin tux is walking, do the special sliding
    if (global.powerup = cs_shell or global.powerup = cs_penguin)
    and state = 1
        with instance_create(x,y,obj_slider) event_user(0)

    //If tux is on a slope, and the above didn't happen, slide normally
    else if collision_point(bbox_right,bbox_bottom+1,obj_slopeparent,1,0)
    xor collision_point(bbox_left,bbox_bottom+1,obj_slopeparent,1,0)
        instance_create(x,y,obj_slider)

}

//Kuribo shoes give tux the special stomp ability at all times
if global.yoshi = 2
    stompstyle = 1

//Disable spin jumping if tux is not jumping, or holding something
else if stompstyle = 1
{

    if state != 2
    or holding != 0
        stompstyle = 0

}

//If tux is jumping, not ducking, not spin jumping, can control himself, and is not riding anything
if state = 2
and isduck = 0
and stompstyle = 0
and disablecontrols = 0
and global.yoshi = 0
{

    //Handles cape and tanooki tux's flying
    if (global.powerup = cs_cape or global.powerup = cs_tanooki)
    and keyboard_check_pressed(vk_shift)
    and jumpnow != 1
    {

        //Fly if the P-meter is flashing and float otherwise
        if obj_levelmanager.pmeter &gt; 5
            vspeed = -4
        else
            vspeed = -0.5

        //Wiggle animation
        wiggle = 16

        //Sound effect
        audio_play_sound(snd_spin,0,0)

    }

    //Handles checking whether tux can wall climb or wall jump
    if (global.powerup = cs_ninja or global.powerup = cs_walljump)
    and vspeed &gt; 0
    and holding = 0
    {

        //If the direction tux is facing is pressed, while a wall is in front of him
        if (keyboard_check(vk_right)
        and collision_rectangle(bbox_right,bbox_top+4,bbox_right+1,bbox_bottom-1,obj_solid,1,0)
        and direct = 1)
        or (keyboard_check(vk_left)
        and collision_rectangle(bbox_left-1,bbox_top+4,bbox_left,bbox_bottom-1,obj_solid,1,0)
        and direct = -1)
        {

            //Make tux start wall climbing/jumping
            state = 4

            //Stop all vertical movement
            vspeed = 0

            //Disable gravity
            gravity = 0

            //Disable carrot and bee tux floating
            isfloating = 0

            //Change directions if not ninja climbing
            if global.powerup != cs_ninja
                direct = -direct

            //Stop carrot/bee tux's floating sound
            if floatnow = 1
            {
                audio_stop_sound(snd_spin)
                floatnow = 0
            }

            //He wall jumped
            walljump = 1

        }

    }

    //Handles carrot and bee tux's floating
    if (global.powerup = cs_carrot or (global.powerup = cs_bee and beefly &lt; 128))
    and keyboard_check(vk_shift)
    {

        //If tux is moving downwards
        if vspeed &gt; 0
        {

            //Set the floating variable
            isfloating = 1

            //Bee tux flies upwards if the ceiling is not above him
            if global.powerup = cs_bee
            and not collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_top,obj_solid,1,0)
            {

                //Fly upwards
                vspeed = -1.5

                //Make the bee timer tick extra because of the initial push
                beefly += 8

            }

            //Carrot tux floats down slowly
            else
                vspeed = 0.5

        }

        //If tux is not moving downwards
        else
        {

            //If bee tux is floating
            if global.powerup = cs_bee
            and isfloating = 1
            {

                //Continue flying upwards
                vspeed = -1.5

                //Make the bee timer tick
                beefly++

                //Slow bee tux down if he is moving too fast
                if hspeed &gt; 1
                    hspeed -= 0.1
                if hspeed &lt; -1
                    hspeed += 0.1

            }

            //Stop floating otherwise
            else
                isfloating = 0

        }

    }

    //Stop floating
    else
        isfloating = 0

}

//Stop floating
else
    isfloating = 0

//If carrot or bee tux is floating, but the sound is not playing
if isfloating = 1
and floatnow = 0
{

    //Mark that the sound is playing
    floatnow = 1

    //Loop the sound
    audio_play_sound(snd_spin,0,1)

}

//If carrot or bee tux is not floating, but the sound is playing
else if isfloating = 0
and floatnow = 1
{

    //Mark that the sound is not playing
    floatnow = 0

    //Stop the sound
    audio_stop_sound(snd_spin)

}
