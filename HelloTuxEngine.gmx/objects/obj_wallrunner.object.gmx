            <string>///Prepare tux to stop wallrunning

//If tux exists
if instance_exists(obj_tux)
{

    //Make him visible and enable controls
    with obj_tux
    {
        visible = 1
        disablecontrols = 0
    }

    //If tux is running up, use the regular direction
    if direction = 90
        obj_tux.direct = direct

    //Otherwise, reverse his facing direction
    else
        obj_tux.direct = -direct

}
