        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle wall running logic

//Destroy self if tux does not exist
if not instance_exists(obj_tux)
{
    instance_destroy()
    exit
}

//Use the run sprite
sprite_index = scr_tuxrun()

//Snap tux to self
obj_tux.x = x
obj_tux.y = y-2
obj_tux.speed = 0

//If the jump key is pressed
if keyboard_check_pressed(vk_shift)
{

    //Play the jump sound
    audio_play_sound(snd_jump,0,0)

    //If tux is running up, jump off the wall
    if direction = 90
    {
        obj_tux.vspeed = vspeed
        obj_tux.hspeed = -3*direct
    }

    //If tux is running down, jump off the wall
    else if direction = 270
    {
        obj_tux.vspeed = vspeed
        obj_tux.hspeed = 3*direct
    }

    //If tux is running on the ceiling, jump off
    else
    {
        obj_tux.hspeed = hspeed
        obj_tux.vspeed = 3
    }

    //Destroy
    instance_destroy()

}

//If tux is running around clockwise
else if direct = 1
{

    //Speed up if the run and right key is being pressed
    if keyboard_check(vk_control)
    and keyboard_check(vk_right)
        speed += 0.1

    //Otherwise, slow down
    else
        speed -= 0.05

    //If tux is running up a wall on the right
    if direction = 90
    {

        //Destroy self if there is no solid to the right
        if not place_meeting(x+8,y,obj_solid)
        {
            obj_tux.hspeed = -vspeed
            instance_destroy()
        }

    }

    //If tux is running left on a ceiling
    else if direction = 180
    {

        //Destroy self if there is no solid above
        if not place_meeting(x,y-8,obj_solid)
        {
            obj_tux.hspeed = hspeed
            instance_destroy()
        }

        //Check for an inverted right wall triangle
        var col = instance_position(x,y-8,obj_walltriangle_ir)
        if col
        {
            x = col.x-16
            y = col.y
            direction = 90
        }

    }

    //If tux is running down a wall on the left
    else if direction = 270
    {

        //Destroy self if there is no solid to the left
        if not place_meeting(x-8,y,obj_solid)
        {
            obj_tux.vspeed = vspeed
            instance_destroy()
        }

        //Check for an inverted left wall triangle
        var col = instance_position(x-24,y+16,obj_walltriangle_il)
        if col
        {
            x = col.x
            y = col.y+16
            direction = 180
        }

    }

}

//If tux is running around counterclockwise
else if direct = -1
{

    //Speed up if the run and left key is being pressed
    if keyboard_check(vk_control)
    and keyboard_check(vk_left)
        speed += 0.1

    //Otherwise, slow down
    else
        speed -= 0.05

    //If tux is running up a wall on the left
    if direction = 90
    {

        //Destroy self if there is no solid to the left
        if not place_meeting(x-8,y,obj_solid)
        {
            obj_tux.hspeed = vspeed
            instance_destroy()
        }

    }

    //If tux is running right on a ceiling
    else if direction = 0
    {

        //Destroy self if there is no solid above
        if not place_meeting(x,y-8,obj_solid)
        {
            obj_tux.hspeed = hspeed
            instance_destroy()
        }

        //Check for an inverted left wall triangle
        var col = instance_position(x,y-8,obj_walltriangle_il)
        if col
        {
            x = col.x+32
            y = col.y
            direction = 90
        }

    }

    //If tux is running down a wall on the right
    else if direction = 270
    {

        //Destroy self if there is no solid to the right
        if not place_meeting(x+8,y,obj_solid)
        {
            obj_tux.vspeed = vspeed
            instance_destroy()
        }

        //Check for an inverted right wall triangle
        var col = instance_position(x+24,y+16,obj_walltriangle_ir)
        if col
        {
            x = col.x+16
            y = col.y+16
            direction = 0
        }

    }

}

//Destroy self if tux is too slow, or runs into a solid
if speed &lt; 1.6
or place_meeting(x,y,obj_solid)
{
    obj_tux.speed = speed
    obj_tux.direction = direction
    instance_destroy()
}

//Stop tux from running too fast
else if speed &gt; 3
    speed = 3
</string>
